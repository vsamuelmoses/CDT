    <meta http-equiv="Cache-Control" content="no-cache">
    <meta http-equiv="Expires" content="Sat, 01 Dec 2021 00:00:00 GMT">
    <meta property="og:url" content="http://cardrivingtheory.co.uk/" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="DVSA Driving Theory" />
    <meta property="og:description" content="Practise latest and revised official DVSA driving theory questions. Revise all questions unlimited times and prepare for passing your driving theory."/>
    <meta name="description" content="Practise latest and revised official DVSA driving theory questions. Revise all questions unlimited times and prepare for passing your driving theory.">
    <title>Car Driving Theory</title>

    @Html.Partial(MVC.Shared.Views.VendorResourses)
    @Html.Partial(MVC.Shared.Views.Urls)
    @Html.Partial(MVC.Shared.Views.Assets)


    @RenderSection("scripts", required: false)
    <script type='text/javascript' src='//cdnjs.cloudflare.com/ajax/libs/angular-loading-bar/0.7.1/loading-bar.min.js'></script>
    <script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
