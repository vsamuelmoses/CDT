    ga('send', 'pageview');

    </script>

</head>
<body ng-app="app">
    <div class="main-wrapper">
        <div class="nav-wrapper">
            <nav class="navbar navbar-static-top" role="navigation">
                <div class="navbar-header">
                    <a href="#" class="logo">
                        <img src="~/Scripts/lib/template-files/img/logo-car.png" alt="Driving Theory" />
                    </a>
                </div>
                <ul class="nav navbar-top-links navbar-right ng-scope" ng-controller="navLoginController">
                    <li>
                        <a href="https://www.facebook.com/cardrivingtheoryuk/">
                            <i class="fa fa-facebook"></i>
                        </a>
                    </li>
                    
                    <li>
                        <a href="" ui-sref="dashboard">
                            <i class="fa fa-home"></i>
                        </a>
                    </li>
                    <li ng-if="loggedUser">
                        <a ui-sref="accountSettings"
                           href="">
                            <i class="fa fa-user"></i> <span class="text">{{loggedUser.firstName}}</span>
                        </a>
                    </li>
                    <li ng-if="loggedUser">
                        <a class="logout-lnk" href="" ng-click="logOff()">
                            <i class="fa fa-sign-out"></i> <span class="text">Log out</span>
                        </a>
                    </li>
                    <li>
                        <a href="">
                            <span class="btn btn-primary" data-target="#side-menu" data-toggle="collapse" ng-click="toggleNav()"><i class="fa fa-bars"></i> </span>

                        </a>
                    </li>
                </ul>
            </nav>
        </div>

        <div id="wrapper">
            <nav class="navbar-default navbar-static-side ng-scope collapse navbar-collapse" ng-controller="topicsMenuController" id="side-menu" >
                <ul class="nav metismenu aside">

                    <li ng-class="{'active': t.topic.id === selectedTopic.id}" ng-repeat="t in topics">
                        <a href="javascript:void(0)" ng-click="selectTopic(t.topic)">
                            <div class="nav-label crop">
                                <b>{{t.topic.name}}</b>
                            </div>
                            <br />
                            <div ng-show="loggedUser">
                                <div class="progress progress-mini">
                                    <div ng-style="{ 'width' : t.progress + '%' }" class="progress-bar"></div>
                                </div>
                            </div>
                        </a>
                    </li>
                </ul>
            </nav>
            <div id="page-wrapper" class="gray-bg">
                @RenderBody()
            </div>
        </div>
    </div>

    
    <div class="footer-row-xs footer">
        
    </div>

